{
  "name": "chances_death-in-nyc",
  "version": "0.0.1",
  "description": "An app to determine the user's probability of type of death based on health services data from 2007 to 2014.",
  "main": "js/index.js",
  "scripts": {
    "start": "node server.js",
    "startLocal": "http-server -c-1 build",
    "dev": "run-p watch serve",
    "devLocal": "run-p watch startLocal",
    "test": "jest",
    "build": "npm run build:html && npm run build:js && npm run build:css && npm run build:assets && npm run build:img",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "npm run clean:html && npm run mkdir:html && npm run _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "npm run clean:js && npm run mkdir:js && npm run _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "npm run clean:css && npm run mkdir:css && npm run _build:css",
    "_build:css": "cpr css build/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:img": "run-s mkdir",
    "clean:img": "rimraf build/img",
    "build:img": "npm run clean:img && npm run mkdir:img && npm run _build:img",
    "_build:img": "cpr img build/img",
    "watch:img": "run-s build:img _watch:img",
    "_watch:img": "chokidar img/**/* -c \"npm run build:img\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "npm run clean:assets && npm run mkdir:assets && npm run _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\"",
    "build:server": "rimraf public && mkdirp public && cpr build public -r",
    "commit": "git add public/* && git commit -m \"Commit public folder for heroku\"",
    "push": "git push heroku master",
    "deploy": "npm run build && npm run build:server && npm run commit && npm run push"
  },
  "author": "Daniel Smither <smitherd9@gmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.3",
    "cpr": "^2.2.0",
    "enzyme": "^2.9.1",
    "http-server": "^0.12.1",
    "install-peers": "^1.0.3",
    "jest-cli": "^18.1.0",
    "js-mock": "^1.0.2",
    "jsdom": "9.9.1",
    "jsdom-global": "2.1.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.3",
    "mocha-jsdom": "^1.2.0",
    "npm-run-all": "^3.0.0",
    "rimraf": "^2.7.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.14.0",
    "webpack": "^1.15.0"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-airbnb": "^2.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "braces": "^3.0.2",
    "chai": "^3.5.0",
    "cors": "^2.8.5",
    "diff": "^4.0.2",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "growl": "^1.10.5",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-alert": "^1.0.14",
    "react-bootstrap": "^0.30.10",
    "react-c3js": "^0.1.20",
    "react-dom": "^15.6.2",
    "react-redux": "^4.4.10",
    "react-router": "^3.2.5",
    "react-select-box": "^3.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "unirest": "^0.6.0"
  }
}
